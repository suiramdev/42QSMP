{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Discord.js",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "paths": {
      "@database/*": [
        "../../packages/database/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
